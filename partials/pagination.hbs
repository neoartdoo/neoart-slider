<div class="page">
    {{#if prev}}
    <a class="ajaxPageLink readMore" href="{{page_url prev}}"><i class="fa fa-angle-left"></i></a>
    {{/if}}
   <span class="pageNumber">Page <span class="curPage">{{page}}</span> of {{pages}}</span>
    {{#if next}}
    <a class="ajaxPageLink readMore" href="{{page_url next}}"><i class="fa fa-angle-right"></i></a>
    {{/if}}
</div>
